; complete
(global-set-key [f5] 'dabbrev-expand)

(defun reload-dot-emacs ()
  (interactive)
  (load-file "~/.emacs")
  (message "Dot Emacs reloaded"))

(global-set-key "\M-r" 'reload-dot-emacs)
(provide 'bindings-def)
